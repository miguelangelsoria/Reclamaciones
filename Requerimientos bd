Se requiere instalar la aplicacion XAMPP que contiene PHPMYADMIN  que usara como base de datos  creando las siguientes tablas.

<?xml version="1.0" encoding="utf-8"?>
<!--
- phpMyAdmin XML Dump
- version 4.8.4
- https://www.phpmyadmin.net
-
- Servidor: localhost
- Tiempo de generación: 04-08-2020 a las 03:38:58
- Versión del servidor: 10.1.37-MariaDB
- Versión de PHP: 7.1.26
-->

<pma_xml_export version="1.0" xmlns:pma="https://www.phpmyadmin.net/some_doc_url/">
    <!--
    - Structure schemas
    -->
    <pma:structure_schemas>
        <pma:database name="reclamaciones" collation="utf8_spanish2_ci" charset="utf8">
            <pma:table name="trabajadores">
                CREATE TABLE `trabajadores` (
                  `id_trabajador` int(11) NOT NULL AUTO_INCREMENT,
                  `nombre` varchar(50) COLLATE utf8_spanish2_ci NOT NULL,
                  `apellido` varchar(50) COLLATE utf8_spanish2_ci NOT NULL,
                  `categoría` varchar(50) COLLATE utf8_spanish2_ci NOT NULL,
                  `fecha de elaboración` date NOT NULL,
                  `fecha de recepcion` date NOT NULL,
                  `conceptos reclamados` int(50) NOT NULL,
                  `nombre del analista` varchar(20) COLLATE utf8_spanish2_ci NOT NULL,
                  `estatus` varchar(50) COLLATE utf8_spanish2_ci NOT NULL,
                  `conceptos incluidos` int(50) NOT NULL,
                  `qna. inclusión` text COLLATE utf8_spanish2_ci NOT NULL,
                  PRIMARY KEY (`id_trabajador`)
                ) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 COLLATE=utf8_spanish2_ci;
            </pma:table>
        </pma:database>
    </pma:structure_schemas>

    <!--
    - Base de datos: 'reclamaciones'
    -->
    <database name="reclamaciones">
        <!-- Tabla trabajadores -->
        <table name="trabajadores">
            <column name="id_trabajador">1</column>
            <column name="nombre"></column>
            <column name="apellido"></column>
            <column name="categoría"></column>
            <column name="fecha de elaboración">0000-00-00</column>
            <column name="fecha de recepcion">0000-00-00</column>
            <column name="conceptos reclamados">0</column>
            <column name="nombre del analista"></column>
            <column name="estatus"></column>
            <column name="conceptos incluidos">0</column>
            <column name="qna. inclusión"></column>
        </table>
        <table name="trabajadores">
            <column name="id_trabajador">2</column>
            <column name="nombre"></column>
            <column name="apellido"></column>
            <column name="categoría"></column>
            <column name="fecha de elaboración">0000-00-00</column>
            <column name="fecha de recepcion">0000-00-00</column>
            <column name="conceptos reclamados">0</column>
            <column name="nombre del analista"></column>
            <column name="estatus"></column>
            <column name="conceptos incluidos">0</column>
            <column name="qna. inclusión"></column>
        </table>
